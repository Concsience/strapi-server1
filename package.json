{
  "name": "strapi-server-2",
  "private": true,
  "version": "0.1.0",
  "description": "A Strapi application",
  "scripts": {
    "develop": "npx strapi develop",
    "start": "npx strapi start",
    "build": "npx strapi build",
    "build:check": "tsc --noEmit",
    "build:production": "NODE_ENV=production npx strapi build",
    "strapi": "npx strapi",
    "deploy": "npx strapi deploy",
    "ts:check": "tsc --noEmit",
    "ts:watch": "tsc --watch",
    "db:indexes": "node scripts/apply-database-indexes.js",
    "db:backup": "bash backup_database.sh",
    "env:validate": "node -e \"require('./src/utils/env-validation.js').validateAndExit()\"",
    "env:generate": "node -e \"console.log(JSON.stringify(require('./src/utils/env-validation.js').generateSecrets(), null, 2))\"",
    "health": "curl -f http://localhost:1337/api/health || echo 'Health check failed'",
    "production:check": "npm run env:validate && echo 'TypeScript check skipped for production' && echo 'Build ready for deployment'",
    "test": "echo 'No tests specified yet'",
    "test:api": "node tests/api/test-suite.js",
    "test:full": "./tests/run-tests.sh",
    "test:performance": "./scripts/test-build.sh"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.798.0",
    "@strapi/plugin-cloud": "5.14.0",
    "@strapi/plugin-users-permissions": "5.14.0",
    "@strapi/provider-email-nodemailer": "^5.14.0",
    "@strapi/provider-upload-aws-s3": "^5.14.0",
    "@strapi/strapi": "5.14.0",
    "@swc/core": "^1.7.26",
    "@types/pg": "^8.15.2",
    "ajv": "^8.17.1",
    "axios": "^1.8.4",
    "express-rate-limit": "^7.5.0",
    "ioredis": "^5.6.1",
    "node-cron": "^3.0.3",
    "pg": "^8.16.0",
    "playwright": "^1.51.1",
    "rate-limit-redis": "^4.2.1",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-router-dom": "^6.0.0",
    "sharp": "^0.34.2",
    "strapi-plugin-populate-deep": "^3.0.1",
    "stripe": "^17.5.0",
    "styled-components": "^6.0.0",
    "xml2js": "^0.6.2",
    "xmldom": "^0.6.0"
  },
  "author": {
    "name": "A Strapi developer"
  },
  "strapi": {
    "uuid": "96dbe8a4-b816-4ea4-8b7a-5cc4382dff7c"
  },
  "engines": {
    "node": ">=18.0.0 <=20.x.x",
    "npm": ">=6.0.0"
  },
  "license": "MIT",
  "devDependencies": {
    "@modelcontextprotocol/server-filesystem": "^2025.3.28",
    "@modelcontextprotocol/server-postgres": "^0.6.2",
    "@strapi/typescript-utils": "5.14.0",
    "@types/node": "^20.17.9",
    "typescript": "^5.2.2"
  }
}
