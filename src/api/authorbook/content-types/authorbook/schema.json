{
  "kind": "collectionType",
  "collectionName": "authorbooks",
  "info": {
    "singularName": "authorbook",
    "pluralName": "authorbooks",
    "displayName": "authorbook",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "bookImage": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "bookTitle": {
      "type": "string"
    },
    "author": {
      "type": "string"
    },
    "quote": {
      "type": "string"
    },
    "epoque": {
      "type": "string"
    },
    "language": {
      "type": "string"
    },
    "Reliure": {
      "type": "string"
    },
    "ISBN": {
      "type": "string"
    },
    "Datedeparution": {
      "type": "string"
    },
    "Couverture": {
      "type": "string"
    },
    "page": {
      "type": "string"
    },
    "format": {
      "type": "string"
    },
    "papier": {
      "type": "string"
    },
    "list_collection": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::list-collection.list-collection",
      "inversedBy": "authorbooks"
    },
    "price": {
      "type": "integer"
    },
    "description": {
      "displayName": "descriptionBook",
      "type": "component",
      "repeatable": false,
      "component": "descriptions.description-book"
    },
    "illustrator": {
      "type": "string"
    },
    "cart_items": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::cart-item.cart-item",
      "mappedBy": "book"
    },
    "ordered_items": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::ordered-item.ordered-item",
      "mappedBy": "book"
    }
