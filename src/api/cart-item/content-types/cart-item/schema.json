    },
    "artistname": {
      "type": "string"
    },
    "paper_type": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::paper-type.paper-type",
      "inversedBy": "cart_items"
    },
    "price": {
      "type": "decimal",
      "required": true,
      "min": 0
    },
    "book": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::authorbook.authorbook",
      "inversedBy": "cart_items"
    },
    "book_title": {
      "type": "string"
    },
    "author_name": {
      "type": "string"
    },
    "quantity": {
      "type": "integer",
      "required": true,
      "min": 1,
      "max": 100,
      "default": 1
    },
    "total_price": {
      "type": "decimal",
      "required": true,
      "min": 0
    }
  }
