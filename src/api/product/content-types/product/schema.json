{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "product",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "productimage": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "productprice": {
      "type": "string"
    },
    "productdescription": {
      "type": "text"
    },
    "orders": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::order.order",
      "mappedBy": "products"
    },
    "favorites": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::favorite.favorite",
      "mappedBy": "products"
    },
    "productitle": {
      "type": "string"
    }
  }
}
