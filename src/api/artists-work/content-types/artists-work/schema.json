{
  "kind": "collectionType",
  "collectionName": "artists_works",
  "info": {
    "singularName": "artists-work",
    "pluralName": "artists-works",
    "displayName": "arts",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "artimage": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "artname": {
      "type": "string"
    },
    "artist": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::artist.artist",
      "inversedBy": "art"
    },
    "popularityscore": {
      "type": "integer"
    },
    "cart_items": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::cart-item.cart-item",
      "mappedBy": "art"
    },
    "ordered_items": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::ordered-item.ordered-item",
      "mappedBy": "art"
    },
    "original_width": {
      "type": "decimal"
    },
    "original_height": {
      "type": "decimal"
    },
    "base_price_per_cm_square": {
      "type": "decimal"
    },
    "productsheet": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::productsheet1.productsheet1",
      "inversedBy": "art"
    },
    "max_size": {
      "type": "string"
    },
    "wishlists": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::wishlist.wishlist",
      "mappedBy": "arts"
    },
    "artThumbnail": {
      "type": "string"
    }
  }
