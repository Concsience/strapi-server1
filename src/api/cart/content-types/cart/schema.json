{
  "kind": "collectionType",
  "collectionName": "carts",
  "info": {
    "singularName": "cart",
    "pluralName": "carts",
    "displayName": "cart",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "user": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "cart"
    },
    "cart_items": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::cart-item.cart-item",
      "mappedBy": "cart"
    },
    "total_price": {
      "type": "decimal",
      "default": 0,
      "min": 0
    },
    "status": {
      "type": "enumeration",
      "enum": ["active", "converted", "abandoned"],
      "default": "active"
    }
  }
}
